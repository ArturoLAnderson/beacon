{
  "name": "chat_p2p",
  "version": "0.1.0",
  "description": "",
  "main": "electron/index.js",
  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=development webpack --mode development",
    "dev:win": "set NODE_ENV=development&&set BABEL_ENV=development&&webpack --mode development",
    "build": "NODE_ENV=production BABEL_ENV=production webpack --mode production",
    "build:win": "set NODE_ENV=production&&set BABEL_ENV=production&&webpack --mode production",
    "serve": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --mode development --host 0.0.0.0 --progress",
    "serve:win": "set NODE_ENV=development&&set BABEL_ENV=development&&webpack-dev-server --mode development --host 0.0.0.0 --progress",
    "start": "concurrently \"npm run serve\" \"wait-on http-get://localhost:2018&&npm run electron:serve:osx\"",
    "start:win": "concurrently \"npm run serve:win\" \"wait-on http-get://localhost:2018&&npm run electron:serve:win\"",
    "rebuild:dev:x64": "electron-rebuild -f -w better-sqlite3",
    "rebuild:dev:ia32": "electron-rebuild -f -a ia32 -w better-sqlite3",
    "electron:init": "cd ./electron/ && npm install && cd ..",
    "electron:serve:osx": "export NODE_ENV=development;electron .",
    "electron:serve:win": "set NODE_ENV=development&&electron .",
    "electron:rebuild:ia32": "electron-rebuild --module-dir ./electron --arch=ia32",
    "electron:rebuild:x64": "electron-rebuild --module-dir ./electron --arch=x64",
    "electron:pack:osx": "electron-packager ./electron/ --out=dist --platform=darwin --arch=x64 --overwrite --icon=assets/ico/app.icns",
    "electron:pack:linux": "electron-packager ./electron/ --out=dist --platform=linux --arch=x64 --overwrite --icon=assets/ico/app.png",
    "electron:pack:win32": "electron-packager ./electron/ --out=dist --platform=win32 --arch=ia32 --overwrite --icon=assets/ico/app.ico",
    "electron:pack:win64": "electron-packager ./electron/ --out=dist --platform=win32 --arch=x64 --overwrite --icon=assets/ico/app.ico",
    "electron:publish:osx": "npm run build && node ./scripts/copy-to-electron.js && npm run electron:init && npm run electron:rebuild:x64 && npm run electron:pack:osx",
    "electron:publish:linux": "npm run build && node ./scripts/copy-to-electron.js && npm run electron:init && npm run electron:rebuild:x64 && npm run electron:pack:linux",
    "electron:publish:win32": "npm run build:win && node ./scripts/copy-to-electron.js && npm run electron:init && npm run electron:rebuild:ia32 && npm run electron:pack:win32",
    "electron:publish:win64": "npm run build:win && node ./scripts/copy-to-electron.js && npm run electron:init && npm run electron:rebuild:x64 && npm run electron:pack:win64",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^4.1.4",
    "blueimp-md5": "^2.10.0",
    "buffer": "^5.2.1",
    "chatroom-client": "file:../chatroom_client",
    "common.communicate": "file:../common/communicate",
    "common.download": "file:../common/download",
    "common.messages": "file:../common/messages",
    "common.ui-components": "file:../common/ui-components",
    "common.utils": "file:../common/utils",
    "crypto-js": "^3.1.9-1",
    "electron-json-storage": "^4.1.4",
    "graphlib": "^2.1.5",
    "hammerjs": "^2.0.8",
    "jimp": "^0.5.4",
    "jquery": "^3.3.1",
    "math": "0.0.3",
    "microtime": "^2.1.8",
    "moment": "^2.22.2",
    "nano-time": "^1.0.0",
    "nkn-client": "^0.4.6",
    "shelljs": "^0.8.2",
    "shortcut-capture": "^1.0.1",
    "slash": "^2.0.0",
    "to-arraybuffer": "^1.0.1",
    "uuid": "^3.3.2",
    "wait-on": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-minify": "^0.5.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-dir": "^0.4.0",
    "css-loader": "^1.0.0",
    "electron": "^3.0.7",
    "electron-packager": "^12.1.1",
    "electron-rebuild": "^1.8.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "style-loader": "^0.22.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
